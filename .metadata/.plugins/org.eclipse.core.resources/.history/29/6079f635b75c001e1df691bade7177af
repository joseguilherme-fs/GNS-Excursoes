
public class Teste {

	public static void main(String[] args) throws Exception {
		Excursão teste = new Excursão(5,25.5,5);
		teste.criarReserva("Guilherme", "953");
		System.out.println(teste);
		teste.criarReserva("Natalia", "953");
		System.out.println(teste);
		teste.criarReserva("Natalia", "953");
		teste.criarReserva("Amanda", "953");
		teste.criarReserva("ABACAXI", "953");
		teste.criarReserva("VACA", "952");
		teste.criarReserva("PUTA", "959");
		System.out.println(teste);
		
		teste.cancelarReserva("953","Natalia");
		System.out.println(teste);
		teste.cancelarReserva("955","Analia");
		System.out.println(teste);
		System.out.println("Antes");
		System.out.println(teste);
		System.out.println();
		System.out.println(teste.listarReservasPorCpf(""));
		System.out.println();
		System.out.println(teste.listarReservasPorNome(""));
		System.out.println(teste.listarReservasPorNome("L"));

	}

}
